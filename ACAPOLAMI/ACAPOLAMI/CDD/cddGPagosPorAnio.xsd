                    <CommandText>SELECT a.nombresConsumidor AS Nombres_del_consumidor, a.apellidosConsumidor AS Apellidos_del_consumidor, a.numeroDocumento AS N_Documento, b.nombreComunidad AS Comunidad, c.monto AS Monto, c.fechaPago AS Fecha_de_pago, d.nombreEstado AS Estado
